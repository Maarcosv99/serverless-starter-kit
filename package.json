{
  "name": "serverless-typescript",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "private": true,
  "scripts": {
    "deploy:dev": "sls deploy --stage dev",
    "deploy:prod": "sls deploy --stage prod",
    "undeploy:prod": "sls remove --stage prod",
    "start:dev": "nodemon --exec npm run offline:dev",
    "start:prod": "nodemon --exec npm run offline:prod",
    "offline:dev": "npx sls offline start --stage dev --host 0.0.0.0 --verbose",
    "offline:prod": "npx sls offline start --stage prod --host 0.0.0.0  --verbose",
    "test:modules": "jest src --runInBand --useStderr --verbose"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.204.0",
    "@middy/core": "^3.4.0",
    "aws-sdk": "^2.1244.0",
    "dynamodb-onetable": "^2.5.1",
    "ulid": "^2.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.6.0",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.2.2",
    "@types/node": "^14.14.25",
    "@types/stringify-object": "^4.0.1",
    "@types/uuid": "^8.3.4",
    "esbuild": "^0.14.11",
    "jest": "^29.2.2",
    "nodemon": "^2.0.20",
    "serverless": "^3.0.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.23.3",
    "serverless-localstack": "^1.0.1",
    "serverless-offline": "^11.2.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^3.2.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
