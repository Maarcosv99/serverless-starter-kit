{
  "extends": "./tsconfig.build.json",
  "compilerOptions": {
    "noUnusedParameters": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": [
      "node",
      "jest",
    ],
  },
  "include": [
    "config/**/*.ts",
    "helpers/**/*.ts",
    "src/**/*.ts",
    "migrations/**/*.ts",
    "serverless.ts"
  ],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*",
  ],
}